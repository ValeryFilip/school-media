  <section id="price">
    <PriceBlock {...priceProps} />
  </section>

  <section id="offer">
    <CourseOffer
      id={offerProps.id}
      badges={offerProps.badges}
      title={offerProps.title}
      subtitle={offerProps.subtitle}
      bullets={offerProps.bullets}
      price={offerProps.price}
      oldPrice={offerProps.oldPrice}
      buy={offerProps.buy}
